   "source": [
    "#first looking at the distribution of all number of children\n",
    "num_child = data['num_child']\n",
    "\n",
    "plt.xlabel(\"Number of Children\")\n",
    "plt.ylabel(\"Count\")\n",
    "plt.title(\"Distribution of the number of children of all mothers\")\n",
    "num_child.hist()\n",
